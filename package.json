{
  "name": "jeedom",
  "version": "0.5.2",
  "description": "Bindings for Jeedom API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint",
    "bump": "conventional-bump -p angular",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "test": "NODE_ENV=test NODE_NO_HTTP2=1 JEEDOM_API_KEY=42 jest",
    "test:all": "npm run prepublishOnly",
    "record:test": "rimraf tests/__nock-fixtures__ && NODE_ENV=test NODE_NO_HTTP2=1 jest -u && node scripts/replace-apikey.js",
    "lint": "npm run test:format && tslint -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test:format": "prettier -l \"src/**/*.{ts,tsx,css}\"",
    "cz": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run -s test:format"
    }
  },
  "keywords": [
    "jeedom",
    "api",
    "bindings",
    "typescript",
    "typings",
    "types",
    "home-automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/jeedom-node.git"
  },
  "author": "Guillaume ARM",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.2",
    "@types/ramda": "^0.26.0",
    "@types/superagent": "^3.8.4",
    "commitizen": "^3.0.4",
    "conventional-bump": "^1.0.6",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "nock-record": "^0.3.3",
    "prettier": "^1.14.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-station": "^0.5.0",
    "typescript": "^3.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "superagent": "^4.0.0-beta.5"
  }
}
