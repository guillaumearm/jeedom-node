{
  "name": "jeedom",
  "version": "0.1.0",
  "description": "Bindings for Jeedom API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "prepublishOnly": "npm run build && npm run test && npm run lint",
    "test": "NODE_ENV=test jest",
    "test:all": "npm run prepublishOnly",
    "lint": "npm run test:format && tslint -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test:format": "prettier -l \"src/**/*.{ts,tsx,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run -s test:format"
    }
  },
  "keywords": [
    "jeedom",
    "api",
    "bindings",
    "typescript",
    "typings",
    "types",
    "home-automation"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/ramda": "^0.26.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-station": "^0.5.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
