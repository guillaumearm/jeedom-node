{
  "name": "jeedom",
  "version": "0.1.0",
  "description": "Bindings for Jeedom API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "test": "echo no test specified",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "lint": "npm run test:format && tslint -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test:format": "prettier -l \"src/**/*.{ts,tsx,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run -s test:format"
    }
  },
  "keywords": [
    "jeedom",
    "api",
    "bindings",
    "home-automation"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^10.12.2",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-station": "^0.5.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
